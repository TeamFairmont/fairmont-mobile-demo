    "engine": {
        "version": "Fairmont Mobile Demo",
        "bind": ":8888",
        "tlsEnabled": false,
        "authMode": "simple",
        "prettyOutput": false,
        "docsEnabled": true,
        "advanced": {
            "stubMode": true,
            "stubDelayMs": 50,
            "completeResultExpiration": "30s",
            "completeResultLoopFreq": "5s",
            "debugFormEnabled": true
        }
    },

    "logging": {
        "level": "info"
    },

    "security": {
        "verifyTimeout": 20,
        "groups": [{
            "name": "engineadmin",
            "hmackey": "admin_key_here"
        }, {
            "name": "publicweb",
            "hmackey": "webaccess1",
            "requestsPerSecond": 500
        }, {
            "name": "publicthrottled",
            "hmackey": "throttleaccess1",
            "requestsPerSecond": 10
        }],
        "handlerAccess": [{
          "handler": "/debug-log",
          "allowGroups": ["engineadmin"]
        }, {
          "handler": "/echo/",
          "allowGroups": ["engineadmin"]
        }, {
          "handler": "/get-config",
          "allowGroups": ["engineadmin"]
        }, {
          "handler": "/pending",
          "allowGroups":  ["engineadmin"]
        }, {
          "handler": "/save-config",
          "allowGroups": ["engineadmin"]
        }, {
          "handler": "/stats",
          "allowGroups": ["engineadmin"]
        }],
        "corsDomains":["http://localhost:8080"],
        "corsAutoAddLocal": true
    },

    "workerConfig": {
        "someGlobalDbInfo": {
            "host": "localhost",
            "port": 5359,
            "user": "write",
            "pass": "12345"
        },
        "cdn": {
            "base":"https://placekitten.com/"
        }
    }
}

